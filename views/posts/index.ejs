<%- include('../partials/header') %>

<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
   <div id="container">
    <h1 id="mb-4">Community Posts</h1>
    
    <% posts.forEach(post => { %>
      <div id="card">
        
        <%= post.title %>
        <a href="/posts/<%= post._id %>"  class="btn btn-success">More</a>
      </div>
      
    <% }) %>
   </div>
   
    
  </body>
</html>

<%- include('../partials/footer') %>

